{
    "name": "adrianbarbos/mobilpay",
    "description": "Laravel 5 mobilpay wrapper around omnipay with omnipay-mobilpay driver",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "adrianbarbos",
            "email": "ady.barbos@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/spgwebdev/omnipay-mobilpay"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "6.0",
        "omnipay/common": "^3@beta",
	"php-http/guzzle6-adapter" : "^1.1",
        "business-mastery/omnipay-mobilpay": "dev-omnipay_3"
    },

    "autoload" : {
        "psr-4": {
            "App\\": "app/",
            "Adrianbarbos\\Mobilpay\\": "src/"
        }
    },

    "minimum-stability": "stable"
}
